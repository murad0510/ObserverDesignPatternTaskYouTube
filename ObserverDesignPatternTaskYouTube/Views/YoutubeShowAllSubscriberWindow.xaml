<Window x:Class="ObserverDesignPatternTaskYouTube.Views.YoutubeShowAllSubscriberWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ObserverDesignPatternTaskYouTube.Views"
        mc:Ignorable="d"
        Title="YoutubeShowAllSubscriberWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush>
                <GradientStop Color="DeepSkyBlue" Offset="0.5"></GradientStop>
            </LinearGradientBrush>
        </Grid.Background>
        <TextBlock Width="500" Name="MyTxtBlock" Height="400" Background="Blue" Text="{Binding TxtBlockContent,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" FontSize="30"></TextBlock>
    </Grid>
</Window>
