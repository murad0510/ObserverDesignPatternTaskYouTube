<Window x:Class="ObserverDesignPatternTaskYouTube.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ObserverDesignPatternTaskYouTube"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas>
            <Button Canvas.Top="90" Canvas.Left="180" Width="450" Height="100" Command="{Binding SubscriberButtonClicked}" Content="Subscriber" FontSize="40"></Button>
            <Button Canvas.Bottom="90" Canvas.Left="180" Content="YouTube" Command="{Binding YouTubeButtonClicked}" FontSize="40" Width="450" Height="100" Foreground="Red"></Button>
        </Canvas>
    </Grid>
</Window>
