<Window x:Class="ObserverDesignPatternTaskYouTube.Views.YouTubeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ObserverDesignPatternTaskYouTube.Views"
        mc:Ignorable="d"
        Title="YouTubeWindow" Height="450" Width="800">
    <Grid>
        <StackPanel x:Name="YoutubeShowAllSubscriberWindow">
            <Canvas>
                <Button Command="{Binding AddNewPost}" Canvas.Top="90" Canvas.Left="190" Width="400" Content="Add new post" FontSize="40" FontFamily="Comic Sans MS" Height="100"></Button>
                <Button Command="{Binding ShowAllSubscriber}" Canvas.Top="250" Canvas.Left="190" Width="400" Content="Show all subscriber" FontSize="40" FontFamily="Comic Sans MS" Height="100"></Button>
            </Canvas>
        </StackPanel>
    </Grid>
</Window>
