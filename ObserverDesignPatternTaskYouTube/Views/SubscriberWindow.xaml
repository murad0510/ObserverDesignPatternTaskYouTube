<Window x:Class="ObserverDesignPatternTaskYouTube.Views.SubscriberWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ObserverDesignPatternTaskYouTube.Views"
        mc:Ignorable="d"
        Title="SubscriberWindow" Height="500" Width="900">
    <Grid>
        <StackPanel x:Name="MyStackPanel" Height="500" Width="900">
            <Canvas>
                <Button Width="400" Height="100" Command="{Binding SignInButtonClicked}" Canvas.Top="90" Canvas.Left="190" Content="Sign in" FontSize="40"></Button>
                <Button Width="400" Height="100" Canvas.Top="250" Command="{Binding RegisterButtonClicked}" Canvas.Left="190" Content="Register" FontSize="40"></Button>
            </Canvas>
        </StackPanel>
    </Grid>
</Window>
